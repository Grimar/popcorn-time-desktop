{
  "presets": [
    ["env", {
      "targets": { "node": 10 },
      "useBuiltIns": true
    }],
    "stage-0",
    "react"
  ],
  "plugins": [
    "add-module-exports"
  ],
  "env": {
    "production": {
      "presets": ["react-optimize"],
      "plugins": [
        "dev-expression",
        ["react-remove-properties", { "properties": ["data-e2e"] }]
      ]
    },
    "development": {
      "plugins": [
        "transform-class-properties",
        "transform-es2015-classes"
        // ["flow-runtime", {
        //   "assert": true,
        //   "annotate": true
        // }]
      ]
    }
  }
}
